<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.1//EN" "http://www.w3.org/TR/xhtml11/DTD/xhtml11.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>首页</title>
    <script src="/asset/js/react.min.js"></script>
	<script src="/asset/js/react-dom.min.js"></script>
	<script src="/asset/js/babel.min.js"></script>
	<script src="/asset/js/angular.min.js"></script>
	<script src="/asset/js/vue.2.5.16.js"></script>
  </head>
  <body>
 	<div id="example"></div>
 	<div ng-app="" ng-init="firstName='John'">
    	<p>在输入框中尝试输入:</p>
    	<p>姓名: <input type="text" ng-model="firstName"></p>
    	<p>你输入的为: {{ firstName }}</p>
    </div>
    <div id = "app">
 		<p style = "font-size:25px;">计数器: {{ counter }}</p>
 		<button @click = "counter++" style = "font-size:25px;">点我</button>
	</div>
 	<script type="text/babel" src="/home/js/home.js"></script>
  </body>
</html>
